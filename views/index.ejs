<%- include('./templates/head') %>
<body>
    <header><h1>Mi agenda de teléfonos</h1></header>
    <main>
        <table id="contactos" class="display" style="width:100%">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Apellido</th>
                    <th>Teléfono</th>
                    <th>Tipo</th>
                </tr>
            </thead>
            <tbody>
            <% datos.forEach(contacto => { %>
                <tr>
                  <td><%= contacto.nombre %></td>
                  <td><%= contacto.apellido %></td>
                  <td><%= contacto.telefono %></td>
                  <td><%= contacto.tipo %></tr></td>  
                    </tr>                     
            <% }) %>
                </tbody>
